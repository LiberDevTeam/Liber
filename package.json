  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js|ts|tsx}": "yarn lint:js --cache --fix",
    "*.{js,ts,tsx,css,scss,sass,md}": "prettier --write",
    "*.{sol}": "solhint --fix"
  },
  "main": "src/index.js",
  "msw": {
    "workerDirectory": "public"
  },
  "private": true,
  "scripts": {
    "build:i18next": "cross-env NODE_ENV=development babel './src/**/*.tsx'",
    "build:storybook": "build-storybook -s public",
    "build:types": "npx typechain --target truffle-v5 --out-dir @types-typechain ./build/contracts/*.json && prettier --write @types-typechain",
    "create:shareddb": "node ./scripts/create-shared-db.js",
    "lint": "eslint src/",
    "build:app": "node ./esbuild/build.js",
    "test": "jest",
    "lint:js": "eslint ./",
    "lint:sol": "solhint --fix ./contracts/**/*.sol",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "postinstall": "npx truffle compile && yarn run build:types",
    "prepare": "husky install",
    "start": "run-p watch:app watch:sw",
    "storybook": "start-storybook -p 6006 -s public",
    "test:js": "jest",
    "test:sol": "tsc -p ./tsconfig.test.json --outDir ./test && truffle test",
    "typecheck": "tsc --noEmit",
    "watch:app": "node ./esbuild/dev.js",
    "watch:sw": "node ./esbuild/sw.js",
    "watch:worker": "node ./esbuild/worker.js"
  }
